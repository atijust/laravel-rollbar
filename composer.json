{
    "name": "atijust/laravel-rollbar",
    "description": "Rollbar integration for Laravel",
    "keywords": ["laravel", "rollbar", "log", "debug", "error", "exception", "tracking", "monitoring"],
    "license": "MIT",
    "authors": [
        {
            "name": "TSUJITA shou",
            "email": "tsujita.shou@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": ">=4.1,<4.4",
        "rollbar/rollbar": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": ">=2.1,<2.4"
    },
    "autoload": {
        "psr-0": {
            "Atijust\\LaravelRollbar": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
